<form method="post" action="{{ path('movie_list_delete', {'id': movie_list.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ movie_list.id) }}">
    <button class="btn btn-success">Delete list</button>
</form>
